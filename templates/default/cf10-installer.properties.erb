#Silent properties for ColdFusion 10
INSTALLER_UI=SILENT
#Valid values are full/trial/developer 
SILENT_LICENSE_MODE=<%= node['cf10']['installer']['license_mode'] %>
SILENT_SERIAL_NUMBER=<%= node['cf10']['installer']['serial_number'] %>
#Use when it is upgrade
SILENT_PREV_SERIAL_NUMBER=<%= node['cf10']['installer']['prev_serial_number'] %>
#Valid values are ear/war/standalone 
SILENT_INSTALLER_TYPE=<%= node['cf10']['installer']['installer_type'] %>
SILENT_INSTALL_ODBC=<%= node['cf10']['installer']['install_odbc'] %>
SILENT_INSTALL_SAMPLES=<%= node['cf10']['installer']['install_samples'] %>
SILENT_INSTALL_JNBRIDGE=<%= node['cf10']['installer']['install_jnbridge'] %>
SILENT_INSTALL_ADMIN=<%= node['cf10']['installer']['install_admin'] %>
SILENT_INSTALL_SOLR=<%= node['cf10']['installer']['install_solr'] %>
SILENT_INSTALL_FOLDER=<%= node['cf10']['installer']['install_folder'].sub(/\\/, "\\\\\\") %>
#Enable secure profile, IP addresses from which Administrator can be accessed
SILENT_ENABLE_SECURE_PROFILE=<%= node['cf10']['installer']['enable_secure_profile'] %>
SILENT_ADMIN_IP=<%= node['cf10']['installer']['admin_ip'] %>
SILENT_ADMIN_USERNAME=<%= node['cf10']['installer']['admin_username'] %>
SILENT_ADMIN_PASSWORD=<%= @admin_password %>
SILENT_ENABLE_RDS=<%= node['cf10']['installer']['enable_rds'] %>
#Provide password when enable RDS is true
SILENT_RDS_PASSWORD=<%= @rds_password %>
#Remote username/password
SILENT_JETTY_USERNAME=<%= node['cf10']['installer']['jetty_username'] %>
SILENT_JETTY_PASSWORD=<%= @jetty_password %>
#Context root for J2EE installation
SILENT_CONTEXT_ROOT=<%= node['cf10']['installer']['context_root'] %>
#Automatically check for serverupdates 
SILENT_AUTO_ENABLE_UPDATES=<%= node['cf10']['installer']['auto_enable_updates'] %>
#Migration (applicable to non-windows OSes only) 
SILENT_MIGRATE_COLDFUSION=<%= node['cf10']['installer']['migrate_coldfusion'] %>
SILENT_PREV_CF_MIGR_DIR=<%= node['cf10']['installer']['prev_cf_migr_dir'] %>
#Runtime User
SILENT_RUNTIME_USER=<%= node['cf10']['installer']['runtimeuser'] %>
